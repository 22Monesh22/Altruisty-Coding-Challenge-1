public class PCalc {
    public static void main(String[] args) {
        int n = 7;
        int[] p = {1, 9, 2, 11, 1, 9, 2};
        int minP = Integer.MAX_VALUE;
        int maxP = 0;
        for (int i = 0; i<n;i++) {
            if (p[i] < minP) {
                minP =p[i];
                
            }
            int profit = p[i] -minP;
            if (profit >maxP) {
                maxP=profit;
            }
        }
        System.out.println(maxP); 
}
